{
    "name": "suzu3",
    "version": "0.1.11",
    "private": true,
    "author": {
        "name": "Suzu Team",
        "email": "emilnakao@gmail.com"
    },
    "build": {
        "appId": "com.suzu3",
        "productName": "SUZU 3",
        "copyright": "Copyright Â© 2020 ${author}",
        "mac": {
            "category": "public.app-category.utilities"
        },
        "files": [
            "build/**/*",
            "node_modules/**/*"
        ],
        "directories": {
            "buildResources": "assets"
        },
        "publish": "github"
    },
    "main": "public/electron.js",
    "repository": "https://github.com/emilnakao/suzu3",
    "homepage": ".",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.26",
        "@fortawesome/free-solid-svg-icons": "^5.12.0",
        "@fortawesome/react-fontawesome": "^0.1.8",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^10.4.7",
        "@testing-library/user-event": "^7.1.2",
        "axios": "^0.19.0",
        "bootstrap": "^4.4.1",
        "csv": "^5.3.1",
        "electron-is-dev": "^1.1.0",
        "electron-log": "^4.2.2",
        "electron-squirrel-startup": "^1.0.0",
        "jquery": "^3.4.1",
        "moment": "^2.24.0",
        "popper.js": "^1.16.0",
        "pouchdb": "^7.1.1",
        "pouchdb-adapter-idb": "^7.1.1",
        "pouchdb-collate": "^7.1.1",
        "pouchdb-core": "^7.1.1",
        "pouchdb-find": "^7.1.1",
        "pouchdb-quick-search": "^1.3.0",
        "react": "^16.12.0",
        "react-bootstrap": "1.0.1",
        "react-day-picker": "^7.4.0",
        "react-dom": "^16.12.0",
        "react-dropzone": "^10.2.1",
        "react-hotkeys": "1.1.4",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.3.0",
        "react-select": "^3.0.8",
        "react-switch": "^5.0.1",
        "react-toastify": "^5.5.0"
    },
    "scripts": {
        "start": "rescripts start",
        "build": "rescripts build",
        "test": "rescripts test --watchAll=false",
        "eject": "react-scripts eject",
        "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
        "postinstall": "electron-builder install-app-deps",
        "preelectron-pack": "yarn build",
        "electron-pack": "electron-builder -mw",
        "prerelease": "yarn build",
        "release": "electron-builder"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@rescripts/cli": "^0.0.13",
        "@rescripts/rescript-env": "^0.0.11",
        "concurrently": "^5.0.2",
        "electron": "^7.1.7",
        "electron-builder": "^22.7.0",
        "enzyme": "^3.11.0",
        "typescript": "^3.7.4",
        "wait-on": "^3.3.0"
    }
}
